<form action="/landmarks/<%= @landmark.id %>" method="post">
  <p><label>Name:</label> <input type="text" name="landmark[name]" id="name" value="<%= @landmark.name %>"></p>
  <p><label>Year Completed:</label> <input type="text" name="landmark[year_completed]" id="year_completed" value="<%= @landmark.year_completed.to_s %>"></input></p>
  <p><input type="submit" value="Edit Landmark"></p>
</form>
